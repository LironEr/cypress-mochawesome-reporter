{
  "name": "cypress-mochawesome-reporter",
  "version": "1.0.3",
  "description": "Zero config Mochawesome reporter for Cypress with screenshots",
  "engines": {
    "node": ">=10"
  },
  "author": "Liron Er",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LironEr/cypress-mochawesome-reporter"
  },
  "main": "lib/reporter.js",
  "bin": {
    "generate-mochawesome-report": "bin/generate-report-cli.js"
  },
  "scripts": {
    "lerna": "lerna",
    "test:prepare": "lerna bootstrap && lerna run --no-bail --scope @example/* serve-test-report",
    "test:run": "cypress run",
    "test:open": "cypress open",
    "prepare-run-test": "npm run test:prepare ; npm run test:run"
  },
  "keywords": [
    "mochawesome",
    "mocha",
    "reporter",
    "cypress"
  ],
  "dependencies": {
    "fs-extra": "^9.0.0",
    "mocha": "^7.1.1",
    "mochawesome": "^6.1.0",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^5.1.0",
    "yargs": "^15.3.1"
  },
  "files": [
    "bin",
    "lib",
    "register.js"
  ],
  "devDependencies": {
    "cypress": "^5.4.0",
    "lerna": "^3.22.1"
  }
}
