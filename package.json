{
  "name": "cypress-mochawesome-reporter",
  "version": "1.3.0",
  "description": "Zero config Mochawesome reporter for Cypress with screenshots",
  "engines": {
    "node": ">=10"
  },
  "author": "Liron Er",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LironEr/cypress-mochawesome-reporter"
  },
  "main": "lib/reporter.js",
  "scripts": {
    "lerna": "lerna",
    "install-examples": "lerna exec --scope @example/* \"npm i\"",
    "test:prepare": "lerna run --stream --no-bail --concurrency 2 --scope @example/* test",
    "test": "cypress run",
    "cypress:open": "cypress open"
  },
  "keywords": [
    "mochawesome",
    "mocha",
    "reporter",
    "cypress"
  ],
  "dependencies": {
    "fs-extra": "^9.0.0",
    "mocha": "^7.1.1",
    "mochawesome": "^6.1.0",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^5.1.0"
  },
  "devDependencies": {
    "cypress": "^6.8.0",
    "lerna": "^3.22.1"
  },
  "peerDependencies": {
    "cypress": ">=6.2.0"
  },
  "files": [
    "lib",
    "register.js",
    "plugin.js"
  ]
}
