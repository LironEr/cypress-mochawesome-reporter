{
  "name": "cypress-mochawesome-reporter",
  "version": "3.0.0",
  "description": "Zero config Mochawesome reporter for Cypress with screenshots",
  "engines": {
    "node": ">=14"
  },
  "author": "Liron Er",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LironEr/cypress-mochawesome-reporter"
  },
  "main": "lib/reporter.js",
  "scripts": {
    "lerna": "lerna",
    "install-examples": "lerna exec --scope @example/* \"npm i\"",
    "test:prepare": "lerna run --stream --no-bail --concurrency 1 --scope @example/* test",
    "test": "cypress run",
    "cypress:open": "cypress open"
  },
  "keywords": [
    "cypress",
    "mochawesome",
    "mocha",
    "reporter",
    "screenshot"
  ],
  "dependencies": {
    "fs-extra": "^10.0.1",
    "mochawesome": "^7.1.2",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.1.1"
  },
  "devDependencies": {
    "cypress": "^9.5.1",
    "lerna": "^4.0.0"
  },
  "peerDependencies": {
    "cypress": ">=6.2.0"
  },
  "files": [
    "lib",
    "register.js",
    "plugin.js"
  ]
}
